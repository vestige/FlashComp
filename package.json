{
  "name": "flash-comp-app",
  "private": true,
  "homepage": "https://vestige.github.io/FlashComp",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "test": "vitest",
    "test:run": "vitest run",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "db:purge": "node scripts/purgeFirestoreData.js",
    "db:purge:yes": "node scripts/purgeFirestoreData.js --yes",
    "db:purge:yes:system": "node scripts/purgeFirestoreData.js --yes --include-system",
    "db:seed": "node scripts/seedFirestoreData.js",
    "db:seed:system": "node scripts/seedFirestoreData.js --include-system",
    "db:reset": "node scripts/purgeFirestoreData.js --yes && node scripts/seedFirestoreData.js",
    "verify:owner-access": "node scripts/verifyOwnerAccess.js"
  },
  "dependencies": {
    "firebase": "^11.9.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "jsdom": "^28.1.0",
    "vite": "^6.3.5",
    "vitest": "^4.0.18"
  }
}
